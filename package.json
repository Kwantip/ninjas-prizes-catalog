{
  "name": "ahhhh",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev --prefix client -- --host\" \"node server/server.js -- --host\"",
    "build": "tsc -b && npm run build --prefix client",
    "lint": "eslint .",
    "preview": "vite preview",
    "start": "concurrently \"npm run serve-client\" \"npm run start-server\"",
    "serve-client": "npx serve client/dist --listen tcp://0.0.0.0:5173",
    "start-server": "node server/server.js"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^7.6.0",
    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "serve": "^14.2.4",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.0.11"
  }
}
